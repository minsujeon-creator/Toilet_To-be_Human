FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    walls
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

    vent
    {
        type            fixedValue;
        value           uniform (0 -10.56 0);   // 40CMH (0.0111 m^3/s) through 0.07 * 0.015 m^2 => velocity â‰ˆ 10.56 m/s
    }

    toilet
    {
        type            noSlip;
    }
    human
    {
        type            noSlip;
    }
    floor
    {
        type            noSlip;
    }
    symmCut
    {
        type            symmetryPlane;
    }
}


